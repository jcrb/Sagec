class Objet_Structure_Pole{
	/**
	 * Identifiant du ple (utilis par la base de donnes)
	 * @var int
	 */
	private $id;
	/**
	 * Code associ au ple
	 * @var string
	 */
	private $code;
	/**
	 * Nom du ple
	 * @var string
	 */
	private $nom;
	
	public function getId(){
		return $this->id;
	}
	public function setId($_id){
		$this->id = $_id;
	}
	
	public function getCode(){
		return $this->id;
	}
	public function setCode($_code){
		$this->code = $_code;
	}
	
	public function getNom(){
		return $this->nom;
	}
	public function setNom($_nom){
		$this->nom = $_nom;
	}
}
?>