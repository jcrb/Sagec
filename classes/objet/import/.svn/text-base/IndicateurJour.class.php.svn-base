class Objet_Import_IndicateurJour{
	/**
	 * Date de l'indicateur
	 * @var int (timestamp)
	 */
	private $jour;
	/**
	 * Nombre de personnes dcds ce jour
	 * @var int
	 */
	private $deces;
	/**
	 * Nombre de personnes ayant plus de 75 ans dcds ce jour
	 * @var int
	 */
	private $deces75a;
	
	public function getJour(){
		return $this->jour;
	}
	public function setJour($_jour){
		$this->jour = $_jour;
	}
	
	public function getDeces(){
		return $this->deces;
	}
	public function setDeces($_deces){
		$this->deces = $_deces;
	}
	
	public function getDeces75a(){
		return $this->deces75a;
	}
	public function setDeces75a($_deces75a){
		$this->deces75a = $_deces75a;
	}
}
?>